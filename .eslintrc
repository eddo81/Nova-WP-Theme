{
	"root": true,
	"parser": "babel-eslint",
	"parserOptions": {
		"sourceType": "module"
	},
	"env": {
		"browser": true,
		"node": true,
		"es6": true
	},
	"extends": "eslint:recommended",
	"plugins": [
		"html"
	],
	"rules": {
		"arrow-parens": 0,
		"generator-star-spacing": 0,
		"no-console": 1,
		"no-unused-vars": 1
	}
}
